<root>
  <DIALOG class="VDialog" name="MainMenu" fullscreen="FALSE" backgroundcolor="51,51,51,200" size="275,600" startPosition="ScreenCenter">

    <!-- button -->
    <control class="VPushButton" pos="10,360" ID="EXAMPLE1_BTN" enabled="true" taborder="5">
      <text text="ExampleButton" font="Arial_18" color="160,160,160,255">
        <statemodifier state="mouseOver" color="200,200,200,255"/>
        <statemodifier state="selected"  color="180,180,180,255"/>
        <statemodifier state="disabled"  color="120,120,120,255"/>
      </text>
    </control>

    <!-- Quit button -->
    <control class="VPushButton" pos="10,440" ID="QUIT" dialogResult="CANCEL" enabled="true" tooltiptext="Quit the application" taborder="6">
      <text text="Quit" font="Arial_18" color="160,160,160,255">
        <statemodifier state="mouseOver" color="200,200,200,255"/>
        <statemodifier state="selected"  color="180,180,180,255"/>
        <statemodifier state="disabled"  color="120,120,120,255"/>
      </text>
    </control>

  </DIALOG>
</root>
